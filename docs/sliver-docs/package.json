{
  "name": "sliver-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prebuild && concurrently -k -n prebuild,dev \"npm run prebuild:watch -- --skip-initial\" \"next dev\"",
    "build": "npm run prebuild && next build",
    "start": "next start",
    "lint": "next lint",
    "prebuild": "node ./prebuild/run-prebuild.js",
    "prebuild:watch": "node ./prebuild/watch.js",
    "offline": "npm run build && zip -r www.zip ./out",
    "postinstall": "node ./prebuild/run-prebuild.js"
  },
  "dependencies": {
    "@fontsource/fira-code": "^5.2.7",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@heroui/react": "^2.8.5",
    "@monaco-editor/react": "^4.7.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "asciinema-player": "^3.11.0",
    "framer-motion": "^12.23.22",
    "fuse.js": "^7.1.0",
    "lunr": "^2.3.9",
    "next": "^15.5.9",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/lunr": "^2.3.7",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}
