app: usql
term: xterm-256
mode: vi
####----####
$if Usql
  $include ken.inputrc
$endif
####----####
vars:
  bell-style: visible
  completion-map-case: true
  completion-query-items: 2000
  convert-meta: false
  editing-mode: vi
binds:
  vi-command:
    d: backward-char
    h: next-history
    n: forward-char
    t: previous-history
